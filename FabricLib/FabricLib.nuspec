<?xml version="1.0"?>
<package >
  <metadata>
    <id>ZBrad.FabricLib</id>
    <version>1.0.0-preview2</version>
    <title>ZBrad's Library for Microsoft Service Fabric</title>
    <authors>zbrad</authors>
    <owners>zbrad</owners>
    <licenseUrl>https://github.com/zbrad/FabLibs/LICENSE</licenseUrl>
    <projectUrl>https://github.com/zbrad/FabLibs</projectUrl>
    <iconUrl>https://github.com/zbrad/FabLibs/ZBrad.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>
      ZBrad's Library for Microsoft Service Fabric
    </summary>
    <description>
      Provides classes for use with Microsoft Service Fabric.
      Currently features for Wcf-based services and Gateways.
      Also includes some helper utilities for creating/removing, starting/stopping
      Fabric services and applications.
    </description>
    <releaseNotes></releaseNotes>
    <copyright>Brad Merrill 2015</copyright>
    <tags>ServiceFabric WCF</tags>
    <dependencies>
      <dependency id="Microsoft.ServiceFabric" version="4.1.260-preview2" />
      <dependency id="Microsoft.ServiceFabric.Data" version="1.1.260-preview2" />
      <dependency id="Microsoft.ServiceFabric.Services" version="1.1.260-preview2" />
      <dependency id="ZBrad.NLogEtw" version="1.0.0-beta" />
    </dependencies>
  </metadata>
  <files>
    <file src="*.dll" target="lib\net45" />
  </files>
</package>
